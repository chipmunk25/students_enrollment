  "scripts": {
    "postbuild": "node postbuild.js",
    "seed": "npx prisma db seed",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate dev",
    "serve": "node --max-old-space-size=2048 -r dotenv/config dist/server/app.js",
    "start": "node --max-old-space-size=2048 -r dotenv/config dist/server/app.js",
    "dev-js-server": "mkdir -p dist && cp -R  src/server dist",
    "dev-webpack-lib": "webpack --mode development --devtool inline-source-map --config webpack.lib.js",
    "dev-webpack-common": "webpack --watch --mode development --devtool inline-source-map --config webpack.common.js",
    "dev": " yarn dev-webpack-lib && concurrently \"yarn dev-js-server\" \"yarn dev-webpack-common\" \"yarn serve\"",
    "build-js-server": "mkdir -p dist && cp -R  src/server dist",
    "build-webpack-lib": "webpack --mode production --config webpack.lib.js",
    "build-webpack-common": "webpack --mode production --config webpack.common.js",
    "build": "yarn build-js-server && yarn build-webpack-lib && yarn build-webpack-common && yarn postbuild "
  },
